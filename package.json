{
  "name": "financier",
  "private": true,
  "version": "1.8.1",
  "releaseDate": "2022-04-24",
  "description": "Straightforward budgeting",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --hot --progress",
    "lint": "eslint --max-warnings=0 src/",
    "format": "prettier --check src/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "start": "yarn server",
    "docs": "jsdoc --configure .jsdoc.json",
    "pull": "docker pull registry.gitlab.com/financier/financier",
    "push": "docker push registry.gitlab.com/financier/financier:production",
    "tag:prod": "docker tag registry.gitlab.com/financier/financier:latest registry.gitlab.com/financier/financier:production",
    "prod": "yarn pull && yarn tag:prod && yarn push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/financier/financier.git"
  },
  "author": "Alexander Harding",
  "bugs": {
    "url": "https://gitlab.com/financier/financier/issues"
  },
  "homepage": "https://gitlab.com/financier/financier",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "angular-mocks": "^1.8.3",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "bundle-loader": "^0.5.6",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.14.0",
    "eslint-plugin-jest": "^26.1.5",
