    "helmet-csp": "^3.4.0",
    "html-loader": "^3.1.0",
    "html-loader-jest": "^0.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "^3.6.10",
    "mini-css-extract-plugin": "^2.6.0",
    "nocache": "^3.0.3",
    "postcss": "^8.4.12",
